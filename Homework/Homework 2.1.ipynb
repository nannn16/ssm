    "c = 1\n",
    "omega = 30\n",
    "k = 1\n",
    "eps = ((dt*c/dh)**2)\n",
    "ks = k*dt/2\n",
    "\n",
    "# Calculate time step j = 1\n",
    "Uxj = np.zeros(101)\n",
    "for i in np.linspace(1, 99, 99, dtype=int):\n",
